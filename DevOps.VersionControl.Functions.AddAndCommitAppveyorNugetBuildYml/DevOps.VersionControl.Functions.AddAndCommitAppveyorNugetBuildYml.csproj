  <ItemGroup>
    <PackageReference Include="CDorst.DevOps.VersionControl.Functions.AddAndCommitGitChanges" Version="2.0.0" />
    <PackageReference Include="CDorst.Metaproject.Cdn.Containers.Build.Helpers" Version="2.0.0" />
  </ItemGroup>

</Project>
