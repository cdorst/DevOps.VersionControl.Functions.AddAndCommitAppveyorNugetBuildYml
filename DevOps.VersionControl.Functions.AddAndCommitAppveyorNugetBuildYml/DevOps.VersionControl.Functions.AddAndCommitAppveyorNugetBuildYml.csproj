    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>cdorst</Authors>
    <Description>Helper for adding and committing an AppVeyor build definition that builds .NET Core projects and deploys .nupkg files to an environment called CDorstNuGet to a repository root directory</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>CDorst.DevOps.VersionControl.Functions.AddAndCommitAppveyorNugetBuildYml</PackageId>
    <Version>1.0.0</Version>
